{
  "name": "require-to-camelcase",
  "displayName": "Require to CamelCase",
  "description": "VS Code extension: Automatically convert require statements to camelCase const declarations",
  "version": "0.0.1",
  "publisher": "Indigo Qiu",
  "author": {
    "name": "Indigo Qiu",
    "url": "https://github.com/HammerRoot"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/HammerRoot/require-to-camelcase.git"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "keywords": [
    "require",
    "import",
    "camelcase",
    "autocomplete",
    "javascript",
    "typescript"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "smartRequireImport.convertToCamelCase",
        "title": "Convert to CamelCase Const Declaration"
      }
    ],
    "keybindings": [
      {
        "command": "smartRequireImport.convertToCamelCase",
        "key": "alt+q",
        "when": "editorTextFocus && resourceLangId =~ /^(javascript|typescript|javascriptreact|typescriptreact)$/"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}
